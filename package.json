	"private": true,
	"type": "system",
	"scripts": {
		"lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
		"npm:refresh": "npm cache clean --force",
		"test:refresh": "npx jest --clearCache",
		"test": "npx jest --no-cache --runInBand --detectOpenHandles --colors --verbose",
		"format": "prettier --config .prettierrc src/**/**.ts --write",
		"typecheck": "tsc --noEmit --jsx react src/**/**.ts*",
		"watch": "cross-env NODE_ENV=development ts-node-dev --respawn --clear --rs ./src/index.tsx",
		"start": "cross-env NODE_ENV=development ts-node-dev ./src/index.tsx"
	},
